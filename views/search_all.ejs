<% include navs/header.ejs %>

<div class="row">
  <form class="col s12" method="POST" action="/search">
    <div class="row">
      <div class="input-field col s6">
        <input name="query" type="text" class="validate">
        <label for="Search">Search</label>
      </div>
    </div>
    <div class="row">
      <button class="waves-effect waves-light btn" id="submit" value="submit" type="submit" >Submit</button>
    </div>
  </form>
</div>
<div class="row">
   <div class="collection">
        <% for(var i=0; i < data.length; i++) { %>
        <a href="//<%=data[i].ip%>:3000/download/<%=data[i].path%>/<%=data[i].file_name%>" class="collection-item"><%=data[i].file_name%></a>
        <% } %>
      </div>
</div>
<% include navs/footer.ejs %>

